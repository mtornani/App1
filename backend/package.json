{
  "name": "radar-smr-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "npx prisma generate && node src/server.js",
    "dev": "nodemon src/server.js",
    "setup": "node src/scripts/setup-directories.js && node src/scripts/setup-models.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:studio": "npx prisma studio",
    "models:download": "node src/utils/modelDownloader.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "prisma": "^5.10.0",
    "@prisma/client": "^5.10.0",
    "@xenova/transformers": "^2.17.0",
    "cheerio": "^1.0.0-rc.12",
    "playwright": "^1.40.0",
    "crawlee": "^3.6.0",
    "pdf-parse": "^1.1.1",
    "fuse.js": "^7.0.0",
    "node-fetch": "^3.3.2",
    "dotenv": "^16.4.5",
    "cron": "^3.1.6",
    "csv-stringify": "^6.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
